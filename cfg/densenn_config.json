{
   "country": "Germany",
   "training":{
      "epochs":50,
      "batch_size":16
   },
   "model":{
      "checkpoint_path":"content/checkpoint/",
      "learning_rate":0.001,
      "tensorboard_dir":"logs/",
      "layers":[
         {
            "index": 0,
            "type":"flatten"
         },
         {
            "index": 1,
            "type":"dense",
            "units":128,
            "activation":"relu"
         },
         {
            "index": 2,
            "type":"dropout",
            "rate":0.2
         },
         {
            "index": 3,
            "type":"dense",
            "units":32,
            "activation":"relu"
         },
         {
            "index": 4,
            "type":"dense",
            "units":1,
            "activation":"linear"
         }
      ],
      "loss":"mse",
      "optimizer":"adam",
      "save_dir":"models"
   },
   "time_steps":10,
   "validation_split":0.3
}
